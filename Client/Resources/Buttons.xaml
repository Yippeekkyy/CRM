<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Canvas x:Key="PanelButtons" >
    
    <Button  Content="Open" Background="Green" Command="{Binding SomeCommand}" Width="60" Height="60"  Margin ="80 40 0 0"/>
    <Button  Content="Close" Background="Red" Command="{Binding SomeCommand}" Width="60" Height="60"  Margin ="180 40 0 0"/>
    <Button  Content="ToPay" Background="Yellow" Command="{Binding SomeCommand}" Width="60" Height="60"  Margin ="280 40 0 0"/>
    <Button  Content="Order" Background="CornflowerBlue" Command="{Binding SomeCommand}" Width="60" Height="60"  Margin ="380 40 0 0" />
    </Canvas>
    

    
    

            
