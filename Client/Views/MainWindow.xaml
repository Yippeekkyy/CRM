<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary Source="/Resources/Buttons.xaml" />
        

    </Window.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="250"/>
        </Grid.ColumnDefinitions>
        <ListBox Grid.Column="0" Background="Black" >
            
            <ListBoxItem  Content="{StaticResource TableStackPanel}" ></ListBoxItem>
            <ListBoxItem  Content="{StaticResource TableStackPanel2}" ></ListBoxItem>
            <ListBoxItem  Content="{StaticResource TableStackPanel3}" ></ListBoxItem>
            <ListBoxItem  Content="{StaticResource TableStackPanel4}" ></ListBoxItem>
            <ListBoxItem  Content="{StaticResource TableStackPanel5}" ></ListBoxItem>


        </ListBox>

            <Canvas  Grid.Column="1" Background="LightYellow" />
        

        
    </Grid>
</Window>
